<core:FragmentDefinition xmlns:core="sap.ui.core" 
  xmlns="sap.m">
  <Label text="Name" labelFor="nameInput"/>
  <Input id="nameInput" required="true" value="{UserModel>/name}"/>

  <Label text="CPF" labelFor="cpfInput"/>
  <Input id="cpfInput" required="true" type="Number" value="{UserModel>/cpf}"/>

  <Label text="Phone Number" labelFor="phoneNumberInput"/>
  <Input id="phoneNumberInput" type="Number" value="{UserModel>/phone}"/>

  <Label text="Birth Date" labelFor="birthDateInput"/>
  <Input id="birthDateInput" type="Date" value="{UserModel>/birthdate}"/>

  <Label text="E-mail" labelFor="emailInput"/>
  <Input id="emailInput" required="true" type="Email" value="{UserModel>/email}"/>

  <Label text="Adress" labelFor="adressInput"/>
  <Input id="adressInput" value="{UserModel>/address}"/>

  <Label text="Number" labelFor="addressNumberInput"/>
  <Input id="addressNumberInput" type="Number" value="{UserModel>/housenumber}"/>

  <Label text="City" labelFor="cityInput"/>
  <Input id="cityInput" value="{UserModel>/city}"/>

  <Label text="State" labelFor="stateInput"/>
  <Input id="stateInput" value="{UserModel>/state}"/>

  <Label text="Country" labelFor="countryInput"/>
  <Input id="countryInput" value="{UserModel>/country}"/>

  <Label text="CEP" labelFor="cepInput"/>
  <Input id="cepInput" type="Number" value="{UserModel>/cep}"/>
</core:FragmentDefinition>